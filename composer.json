{
    "name": "airondev/laravel-cashier-paystack",
    "description": "A Laravel Cashier Wrapper that provides an expressive, fluent interface to Paystack's subscription invoicing services.",
    "keywords": ["laravel", "paystack", "billing", "cashier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Airon Dev",
            "email": "airondev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "dompdf/dompdf": "*",
        "nesbot/carbon": "^1.26.3 || ^2.0",
        "unicodeveloper/laravel-paystack": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Airondev\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Airondev\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Airondev\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
